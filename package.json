    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.2",
    "mocha": "^6.2.0",
    "ts-node": "^8.3.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.5.3"
  },
  "scripts": {
    "build": "tsc",
