{
  "name": "hanguler",
  "version": "0.2.2",
  "description": "Simple Hangul Manipulating Library",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "keywords": [
    "typescript",
    "korean",
    "alphabet",
    "hangul"
  ],
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "devDependencies": {
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.7.7",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
