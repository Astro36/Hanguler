{
  "name": "hanguler",
  "version": "0.2.0",
  "description": "Simple Hangul Manipulating Library",
  "main": "lib/index.js",
  "keywords": [
    "typescript",
    "korean",
    "alphabet",
    "hangul"
  ],
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.6.8",
