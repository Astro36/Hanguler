    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "mocha": "^7.1.0",
    "ts-node": "^8.6.2",
    "typedoc": "^0.16.11",
    "typescript": "^3.8.3"
  },
  "engines": {
    "node": ">=11.0.0"
  },
  "scripts": {
